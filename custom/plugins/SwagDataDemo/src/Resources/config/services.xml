<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!--Start Plugin Definition-->
        <service id="SwagDataDemo\Core\Content\DataDemo\DataDemoDefinition">
            <tag name="shopware.entity.definition" entity="swag_data_demo"/>
        </service>
        <!--End Plugin Definition-->

        <!--Start Translation Definition-->
        <service id="SwagDataDemo\Core\Content\DataDemo\Aggregate\DataDemoTranslation\DataDemoTranslationDefinition">
            <tag name="shopware.entity.definition" entity="swag_data_demo_translation"/>
        </service>
        <!--End Translation Definition-->

        <!--Start Country Extension-->
        <service id="SwagDataDemo\Core\Content\Extension\CountryExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <!--End Country Extension-->

        <!--Start State Extension-->
        <service id="SwagDataDemo\Core\Content\Extension\StateExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <!--End State Extension-->

        <!--Start Language Extension-->
        <service id="SwagDataDemo\Core\Content\Extension\LanguageExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <!--End Language Extension-->

        <!--Start Media Extension-->
        <service id="SwagDataDemo\Core\Content\Extension\MediaExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <!--End Media Extension-->

        <!--Start Product Extension-->
        <service id="SwagDataDemo\Core\Content\Extension\ProductExtension">
            <tag name="shopware.entity.extension" />
        </service>
        <!--End Product Extension-->


    </services>
</container>
